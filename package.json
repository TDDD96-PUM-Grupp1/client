{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "deepstream.io-client-js": "^2.3.0",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "nosleep.js": "^0.7.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-md": "^1.3.0",
    "react-scripts": "1.1.1",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "build-css": "node-sass-chokidar --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && npm run build-css -- --watch --recursive",
    "start": "PORT=8080 && npm-run-all -p watch-css start-js",
    "start-local": "PORT=8080 && REACT_APP_LOCAL=1 && npm-run-all -p watch-css start-js",
    "start-pc": "set PORT=3000 && npm-run-all -p watch-css start-js",
    "start-pc-local": "set PORT=3000 && set REACT_APP_LOCAL=1 && npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --forceExit --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint src/",
    "lintfix": "eslint src/ --fix"
  },
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^1.10.2"
  }
}
